# Environment variables for local development
# Do not commit this file to version control
# Make a copy named .env.local and fill in your actual values

# Database connection strings
MONGODB_URI=mongodb://username:password@host:port/database
MONGODB_ADMIN_URI=mongodb://username:password@host:port/database

# API key for Groq AI
GROQ_API_KEY=your_groq_api_key_here

# Security keys
JWT_SECRET=generate_a_secure_random_string_here
NEXTAUTH_SECRET=generate_a_secure_random_string_here

# API key for cron job authentication - generate using scripts/generate-api-key.js
CRON_API_KEY=generate_a_secure_random_string_here
